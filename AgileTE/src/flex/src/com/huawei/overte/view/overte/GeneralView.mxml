<?xml version="1.0" encoding="utf-8"?>
<mx:Canvas xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx"
		   width="100%" height="100%"
		  initialize="generalctrl.onInit()" 
		  creationComplete="generalctrl.onCreate()"
		   xmlns:control1="com.huawei.overte.control.*">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
		<control1:GeneralCtrl id="generalctrl" page="{this}"/>
	</fx:Declarations>
	<s:TileGroup id="AreaGroup" left="100" right="100" top="100" bottom="100"/>
</mx:Canvas>
